@Object
    inventory_item_img      css     img.inventory_item_img
    inventory_item_name     css     div.inventory_item_name
    inventory_item_price    css     div.inventory_item_price

    inventory_item_img:
        inside      parent
        width       310 px
        height      225 to 270 px
        left-of     inventory_item_name 0 to 40 px

    inventory_item_name:
        inside      parent
        width       ~275px
        height      20 to 40 ox
        below       inventory_item_img  0 to 40 px

    inventory_item_price:
        below inventory_item_name